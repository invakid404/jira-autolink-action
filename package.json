{
  "name": "jira-autolink-action",
  "version": "0.0.0",
  "private": true,
  "description": "Automatically create autolink references to all Jira projects",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"**/*.ts\"",
    "format-check": "prettier --check \"**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/invakid404/jira-autolink-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "invakid404 <invakid404@riseup.net>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.0.0",
    "@octokit/rest": "^19.0.5",
    "jira-client": "^6.22.0",
    "json-to-graphql-query": "^2.1.0",
    "lodash.trim": "^4.5.1"
  },
  "devDependencies": {
    "@types/jira-client": "^6.21.3",
    "@types/lodash.trim": "^4.5.6",
    "@types/node": "^18.11.19",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "2.4.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
